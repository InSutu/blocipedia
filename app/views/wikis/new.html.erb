<%= form_for [@wiki] do |f| %>
  
    <%= f.label :title %>
    <%= f.text_field :title, class: 'form-control', placeholder: "Enter wiki title" %>

    <%= f.label :body %>
    <%= f.text_field :body, class: 'form-control', placeholder: "Enter wiki body" %>

    <%= f.label :private %>
    <%= f.check_box :private, class: 'form-control', placeholder: "Check if Wiki is private" %>    
  <div class="form-group">
    <%= f.submit "Save", class: 'btn btn-success' %>
  </div>
<% end %>